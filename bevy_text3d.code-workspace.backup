          "BEVY_ASSET_ROOT": "${workspaceFolder:bevy_text3d}"
        },
        "cargo": {
          "args": [
            "run",
            "--example",
            "basic",
            "--target-dir",
            "${workspaceFolder:bevy_text3d}/target",
            "--manifest-path",
            "${workspaceFolder:bevy_text3d}/Cargo.toml"
