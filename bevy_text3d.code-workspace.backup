{
  "folders": [
    {
      "path": ".",
      "name": "bevy_text3d"
    }
  ],
  "settings": {
    "[rust]": {
      "editor.defaultFormatter": "rust-lang.rust-analyzer",
      "editor.semanticHighlighting.enabled": false
    },
    "dev.containers.dockerPath": "/usr/bin/podman",
    "docker.dockerPath": "/usr/bin/podman",
    "docker.host": "unix:///run/user/1000/podman/podman.sock",
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.semanticHighlighting.enabled": false,
    "git.autoRepositoryDetection": false,
    "git.detectSubmodules": false,
    "git.detectSubmodulesLimit": 0,
    "git.enableSmartCommit": false,
    "git.scanRepositories": [
      "."
    ],
    "git.smartCommitChanges": "tracked",
    "remote.containers.dockerPath": "/usr/bin/podman",
    "rust-analyzer.files.exclude": [
      "target/",
      "node_modules/",
      "build/",
      "/home/arrel/temp",
      "/home/arrel/gits",
      "/mnt/gits/gits",
      "/home/arrel"
    ],
    "terminal.integrated.env.linux": {
      "RUSTC_WRAPPER": "sccache",
      "WINIT_UNIX_BACKEND": "wayland"
    },
    "workbench.colorCustomizations": {
      "terminal.background": "#36014b69",
      "terminal.foreground": "#058d0c",
      "terminalCursor.background": "#91b101",
      "terminalCursor.foreground": "#268102"
    },
    "workbench.tree.indent": 30
  },
