  "tasks": {
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build Project",
        "type": "shell",
        "command": "cargo",
        "args": [
          "build"
        ],
        "cwd": "${workspaceFolder:bevy_text3d}",
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "echo": true,
          "focus": false,
          "panel": "shared",
          "reveal": "always"
        },
        "problemMatcher": [
          "$rustc"
        ]
      },
      {
        "label": "Run Project",
        "type": "shell",
        "command": "cargo",
        "args": [
          "run"
        ],
        "cwd": "${workspaceFolder:bevy_text3d}",
        "group": "build",
        "presentation": {
          "echo": true,
          "focus": true,
          "panel": "dedicated",
          "reveal": "always"
